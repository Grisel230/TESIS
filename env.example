# Configuración de desarrollo - NO USAR EN PRODUCCIÓN
# Para producción, genera claves seguras con: python -c "import secrets; print(secrets.token_hex(32))"

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True

# Security Keys (CAMBIAR EN PRODUCCIÓN)
# Generar nuevas claves con: python -c "import secrets; print(secrets.token_hex(32))"
SECRET_KEY=tu-clave-secreta-muy-segura-aqui-cambiar-en-produccion
JWT_SECRET_KEY=tu-jwt-secret-key-muy-segura-aqui-cambiar-en-produccion

# Database Configuration
DATABASE_URL=postgresql://postgres:tu_password_aqui@localhost:5432/emociones

# CORS Origins (separados por comas)
CORS_ORIGINS=http://localhost:4200,http://127.0.0.1:4200,http://localhost:4201,http://127.0.0.1:4201

# Rate Limiting (opcional, por defecto usa memoria)
# RATELIMIT_STORAGE_URL=redis://localhost:6379
